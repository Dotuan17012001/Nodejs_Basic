<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Node js basic</title>
    <link rel='stylesheet' href='https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css'>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <div class="topnav">
        <a class="active" href="#home">Home</a>
        <a href="#news">News</a>
        <a href="#contact">Contact</a>
        <a href="#about">About</a>
    </div>

    <!-- <%= dataUser %> -->
    <h2>List User Table</h2>

<div class="content">
    <table>
        <%= test123 %>
        <tr>
          <th>ID</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Action</th>
        </tr>
          <!-- <%= dataUser.length %> -->
          <% for(let i=0; i < dataUser.length; i++) { %>
             <tr>
               <td><%= dataUser[i].id %></td>
               <td><%= dataUser[i].firstName %></td>
               <td><%= dataUser[i].lastName %></td>
               
               <td>
                  <button type="button" class="btn btn-success"> <a href="/detail/user/<%= dataUser[i].id %>" target="_blank">Detail</a> </button>
                  <button type="button" class="btn btn-warning">Edit</button>
                  <button type="button" class="btn btn-danger">Delete</button>
              </td>
               
             </tr>
          <% } %>
      </table>
</div>
      
<script src='https://code.jquery.com/jquery-3.3.1.slim.min.js'></script>
<script src='https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js'></script>
<script src='https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js'></script>     
</body>
</html>